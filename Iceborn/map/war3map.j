            call UnitAddAbilityBJ( 'A008', udg_Some_Unit )
            call SetUnitAbilityLevelSwapped( 'A008', udg_Some_Unit, 6 )
            call UnitRemoveAbilityBJ( 'A008', udg_Some_Unit )
            set bj_forLoopAIndex = bj_forLoopAIndex + 1
        endloop
        set udg_Max_Hp = ModuloInteger(udg_Max_Hp, 10)
