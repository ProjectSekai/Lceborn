function config takes nothing returns nothing
    call SetMapName( "冰裔v1.7" )
    call SetMapDescription( "投入寒冷,迷人的寒霜天地的世界,作为一个霜冻术士,通过值得观察的全部自定义情节,解谜与战斗.单人游戏的RPG的作者为APproject. 2012-12-06 - 2014-04-15." )
    call SetPlayers( 2 )
    call SetTeams( 2 )
    call SetGamePlacement( MAP_PLACEMENT_USE_MAP_SETTINGS )

    call DefineStartLocation( 0, -3968.0, -1472.0 )
    call DefineStartLocation( 1, -7936.0, -1408.0 )

    // Player setup
    call InitCustomPlayerSlots(  )
    call InitCustomTeams(  )
endfunction

