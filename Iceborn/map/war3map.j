        call AddSpecialEffectLocBJ( GetUnitLoc(GetDyingUnit()), "DevilSlam.mdx" )
        call DestroyEffectBJ( GetLastCreatedEffectBJ() )
    else
    endif
    if ( Trig_Death_Effects_Func003C() ) then
        call AddSpecialEffectLocBJ( GetUnitLoc(GetDyingUnit()), "Arcane Nova.mdx" )
        call DestroyEffectBJ( GetLastCreatedEffectBJ() )
        call PlaySoundBJ( gg_snd_DeathCoilSpecialArt1 )
    else
    endif
    if ( Trig_Death_Effects_Func004C() ) then
        call AddSpecialEffectLocBJ( GetUnitLoc(GetDyingUnit()), "Enchantment.mdx" )
        call DestroyEffectBJ( GetLastCreatedEffectBJ() )
        call PlaySoundBJ( gg_snd_Explosion1 )
    else
    endif
    if ( Trig_Death_Effects_Func005C() ) then
        call AddSpecialEffectLocBJ( GetUnitLoc(GetDyingUnit()), "NewGroundEXBlue.mdx" )
        call DestroyEffectBJ( GetLastCreatedEffectBJ() )
        call AddSpecialEffectLocBJ( PolarProjectionBJ(GetUnitLoc(GetDyingUnit()), 75.00, GetUnitFacing(GetDyingUnit())), "NewGroundEXBlue.mdx" )
        call DestroyEffectBJ( GetLastCreatedEffectBJ() )
        call AddSpecialEffectLocBJ( PolarProjectionBJ(GetUnitLoc(GetDyingUnit()), 75.00, ( GetUnitFacing(GetDyingUnit()) + 120.00 )), "NewGroundEXBlue.mdx" )
        call DestroyEffectBJ( GetLastCreatedEffectBJ() )
        call AddSpecialEffectLocBJ( PolarProjectionBJ(GetUnitLoc(GetDyingUnit()), 75.00, ( GetUnitFacing(GetDyingUnit()) - 120.00 )), "NewGroundEXBlue.mdx" )
        call DestroyEffectBJ( GetLastCreatedEffectBJ() )
    else
    endif
    if ( Trig_Death_Effects_Func006C() ) then
        call AddSpecialEffectLocBJ( PolarProjectionBJ(GetUnitLoc(GetDyingUnit()), 50.00, 0.00), "AquaSpike.mdx" )
        call DestroyEffectBJ( GetLastCreatedEffectBJ() )
        call AddSpecialEffectLocBJ( PolarProjectionBJ(GetUnitLoc(GetDyingUnit()), 50.00, 120.00), "AquaSpike.mdx" )
        call DestroyEffectBJ( GetLastCreatedEffectBJ() )
        call AddSpecialEffectLocBJ( PolarProjectionBJ(GetUnitLoc(GetDyingUnit()), 50.00, 240.00), "AquaSpike.mdx" )
        call DestroyEffectBJ( GetLastCreatedEffectBJ() )
    else
    endif
